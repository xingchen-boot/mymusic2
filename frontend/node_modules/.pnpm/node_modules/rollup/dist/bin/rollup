function***REMOVED***prettyBytes(number,***REMOVED***options)***REMOVED***{
	if***REMOVED***(!Number.isFinite(number))***REMOVED***{
		throw***REMOVED***new***REMOVED***TypeError(`Expected***REMOVED***a***REMOVED***finite***REMOVED***number,***REMOVED***got***REMOVED***${typeof***REMOVED***number}:***REMOVED***${number}`);
	}

	options***REMOVED***=***REMOVED***{
		bits:***REMOVED***false,
		binary:***REMOVED***false,
		space:***REMOVED***true,
		...options,
	};

	const***REMOVED***UNITS***REMOVED***=***REMOVED***options.bits
		?***REMOVED***(options.binary***REMOVED***?***REMOVED***BIBIT_UNITS***REMOVED***:***REMOVED***BIT_UNITS)
		:***REMOVED***(options.binary***REMOVED***?***REMOVED***BIBYTE_UNITS***REMOVED***:***REMOVED***BYTE_UNITS);

	const***REMOVED***separator***REMOVED***=***REMOVED***options.space***REMOVED***?***REMOVED***'***REMOVED***'***REMOVED***:***REMOVED***'';

	if***REMOVED***(options.signed***REMOVED***&&***REMOVED***number***REMOVED***===***REMOVED***0)***REMOVED***{
		return***REMOVED***`***REMOVED***0${separator}${UNITS[0]}`;
	}

	const***REMOVED***isNegative***REMOVED***=***REMOVED***number***REMOVED***<***REMOVED***0;
	const***REMOVED***prefix***REMOVED***=***REMOVED***isNegative***REMOVED***?***REMOVED***'-'***REMOVED***:***REMOVED***(options.signed***REMOVED***?***REMOVED***'+'***REMOVED***:***REMOVED***'');

	if***REMOVED***(isNegative)***REMOVED***{
		number***REMOVED***=***REMOVED***-number;
	}

	let***REMOVED***localeOptions;

	if***REMOVED***(options.minimumFractionDigits***REMOVED***!==***REMOVED***undefined)***REMOVED***{
		localeOptions***REMOVED***=***REMOVED***{minimumFractionDigits:***REMOVED***options.minimumFractionDigits};
	}

	if***REMOVED***(options.maximumFractionDigits***REMOVED***!==***REMOVED***undefined)***REMOVED***{
		localeOptions***REMOVED***=***REMOVED***{maximumFractionDigits:***REMOVED***options.maximumFractionDigits,***REMOVED***...localeOptions};
	}

	if***REMOVED***(number***REMOVED***<***REMOVED***1)***REMOVED***{
		const***REMOVED***numberString***REMOVED***=***REMOVED***toLocaleString(number,***REMOVED***options.locale,***REMOVED***localeOptions);
		return***REMOVED***prefix***REMOVED***+***REMOVED***numberString***REMOVED***+***REMOVED***separator***REMOVED***+***REMOVED***UNITS[0];
	}

	const***REMOVED***exponent***REMOVED***=***REMOVED***Math.min(Math.floor(options.binary***REMOVED***?***REMOVED***Math.log(number)***REMOVED***/***REMOVED***Math.log(1024)***REMOVED***:***REMOVED***Math.log10(number)***REMOVED***/***REMOVED***3),***REMOVED***UNITS.length***REMOVED***-***REMOVED***1);
	number***REMOVED***/=***REMOVED***(options.binary***REMOVED***?***REMOVED***1024***REMOVED***:***REMOVED***1000)***REMOVED********REMOVED***exponent;

	if***REMOVED***(!localeOptions)***REMOVED***{
		number***REMOVED***=***REMOVED***number.toPrecision(3);
	}

	const***REMOVED***numberString***REMOVED***=***REMOVED***toLocaleString(Number(number),***REMOVED***options.locale,***REMOVED***localeOptions);

	const***REMOVED***unit***REMOVED***=***REMOVED***UNITS[exponent];

	return***REMOVED***prefix***REMOVED***+***REMOVED***numberString***REMOVED***+***REMOVED***separator***REMOVED***+***REMOVED***unit;
}

